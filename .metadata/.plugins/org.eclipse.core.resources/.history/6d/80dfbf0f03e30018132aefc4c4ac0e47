package test_p9;

public class Sample4_3 
{
	public static void main(String[] args)
	{
		RaccingCar rccar1;
		rccar1 = new RacingCar();
		
		rccar1.newShow();
	}

}
class Car
{
	protected int num;
	protected double gas;
	
	public Car()
	{
		num = 0;
		gas = 0.0;
		System.out.println("生產了車子");
	}
	
	public void setCar(int n,double g)
	{
		num = n;
		gas = g;
		System.out.println("將車號設為"+num+"汽油量設為"+gas);
	}
}